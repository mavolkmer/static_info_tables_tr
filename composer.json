{
  "name": "renolit/static-info-tables-tr",
  "description": "Turkish (tr) language pack for the Static Info Tables providing localized names for countries.",
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Ephraim HÃ¤rer",
      "role": "Developer",
      "email": "ephraim.haerer@renolit.com",
      "homepage": "https://www.renolit.com"
    }
  ],
  "replace": {
    "typo3-ter/static-info-tables-tr": "self.version"
  },
  "repositories": {
    "static-info-tables": {
      "type": "vcs",
	  "url": "https://codeberg.org/mvoehringer/static-info-tables"
    }
  },
  "require": {
    "php": ">=7.2",
    "typo3/cms-core": "^10.4 || ^11.5",
    "sjbr/static-info-tables": "dev-TYPO3-11"
  },
  "autoload": {
    "psr-4": {
      "Renolit\\StaticInfoTablesTr\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "static_info_tables_tr",
      "app-dir": ".Build",
      "web-dir": ".Build/public"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/public/typo3conf/ext/",
      "[ -L .Build/public/typo3conf/ext/static_info_tables_tr ] || ln -snvf ../../../../. .Build/public/typo3conf/ext/static_info_tables_tr"
    ]
  }
}
